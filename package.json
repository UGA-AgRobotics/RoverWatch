{
  "name": "roverwatch",
  "version": "1.0.0",
  "description": "GPS Tracking using Google Maps JS API and Node.js",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "browserify public/js/main.js -o dist/bundle.js",
    "envify-build": "browserify public/js/main.js -t envify > dist/bundle.js",
    "serve": "node server.js",
    "deploy": "npm run build && npm run serve",
    "envify-deploy": "npm run envify-build && npm run serve",
    "dev": "watchify public/js/main.js -o dist/bundle.js"
  },
  "author": "popenc <i.nickpope@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^4.0.0",
    "brfs": "^2.0.0",
    "envify": "^4.1.0",
    "eventemitter2": "^5.0.1",
    "express": "^4.16.2",
    "jquery": "^3.3.1",
    "leaflet": "^1.3.3",
    "leaflet-filelayer": "^1.2.0",
    "popper.js": "^1.14.1",
    "roslib-fork": "^0.20.1",
    "socket.io": "^2.0.4",
    "togeojson": "^0.16.0",
    "utm-latlng": "^1.0.5"
  },
  "devDependencies": {
    "browserify": "^16.2.3",
    "watchify": "^3.11.0"
  }
}
